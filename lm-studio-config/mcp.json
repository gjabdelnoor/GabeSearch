{
  "mcpServers": {
    "gabesearch-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env","SEARX_URL=http://host.docker.internal:8888/search",
        "--env","TOP_K=3",
        "--env","QUERIES=5",
        "--env","PER_PAGE_CHARS=3000",
        "--env","TOTAL_CHARS=15000",
        "mcp-gabesearch-mcp-gabesearch-mcp:latest"
      ]
    }
  }
}
